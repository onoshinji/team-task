<h1>ログイン画面</h1>
<div class="container">
  <%= form_with(scope::session, uril: sessions_path, local: true) do |form| %>
    <div class="form-group">
      <%= form.label :email %>
      <%= form.email_field :email, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= form.label :password %>
      <%= form.password_field :password, class: "form-control" %>
    </div>
  <% end %>
</div>
